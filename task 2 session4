def calculator(a ,b, operation):
  #print=input("enter the operation you want to make on this two numbers: ",operation)
  if operation=="sum":
    return a+b
  elif operation=="subtract":
    return a-b
  elif operation=="multiply":
    return a*b
  elif operation=="divide":
    if b==0:
      return "cannot divide by zero"
    else:
     return a/b
  else:
     return "invalid operation"
print(calculator(10,5,"sum"))
print(calculator(10,5,"subtract"))
print(calculator(10,5,"multiply"))
print(calculator(10,5,"divide"))
print(calculator(10,0,"divide"))
print(calculator(10,2,"modulus"))